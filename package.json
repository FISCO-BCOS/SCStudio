{
	"name": "secsolidity",
	"displayName": "SecSolidity",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "smartide.analyzecontract",
				"title": "SecSolidity: Analyze Contract"
			},
			{
				"command": "smartide.disablerecommand",
				"title": "SecSolidity: Disable Recommand "
			},
			{
				"command": "smartide.enablerecommand",
				"title": "SecSolidity: Enable Recommand: "
			}
		],
		"snippets": [
			{
				"language": "solidity",
				"path": "./src/snippets/solidity.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.12.29",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"extensionDependencies": [
		"JuanBlanco.solidity",
		"philhindle.errorlens"
	],
	"dependencies": {
		"@types/superagent": "^4.1.7",
		"colors.ts": "^1.0.20",
		"superagent": "^5.2.2"
	}
}
