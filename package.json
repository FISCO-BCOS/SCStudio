{
	"name": "scstudio",
	"displayName": "SCStudio",
	"description": "Making Smart Contract Development More Secure and Easier",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"publisher": "Renardbebe",
	"contributors": [
		"Tsinghua-Renardbebe",
		"Tsinghua-fCorleone",
		"Tsinghua-Aurora",
		"WeBank"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/FISCO-BCOS/SCStudio"
	},
	"bugs": {},
	"categories": [
		"Programming Languages",
		"Linters",
		"Testing"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "scstudio.analyzecontract",
				"title": "SCStudio: Analyze Contract"
			},
			{
				"command": "scstudio.analyzeContractWithoutCompile",
				"title": "SCStudio: Analyze Contract Without Compilation"
			},
			{
				"command": "scstudio.disablerecommand",
				"title": "SCStudio: Disable Recommand"
			},
			{
				"command": "scstudio.enablerecommand",
				"title": "SCStudio: Enable Recommand"
			},
			{
				"command": "scstudio.settime",
				"title": "SCStudio: Set Maximum Wait-time for Security Analysis"
			},
			{
				"command": "scstudio.seturl",
				"title": "SCStudio: Set the url of the web service"
			},
			{
				"command": "scstudio.enableWebservice",
				"title": "SCStudio: Enable the web service"
			},
			{
				"command": "scstudio.disableWebservice",
				"title": "SCStudio: Disable the web service"
			}
		],
		"languages": [
			{
				"id": "solidity",
				"aliases": [
					"Solidity",
					"solidity"
				],
				"extensions": [
					".sol"
				],
				"configuration": "./solidity.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "solidity",
				"scopeName": "source.solidity",
				"path": "./syntaxes/solidity.json"
			}
		],
		"configurationDefaults": {
			"[solidity]": {
				"editor.wordBasedSuggestions": true
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.12.29",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"extensionDependencies": [
		"philhindle.errorlens",
		"JuanBlanco.solidity",
		"atishay-jain.all-autocomplete"
	],
	"dependencies": {
		"@types/superagent": "^4.1.7",
		"await-exec": "^0.1.2",
		"colors.ts": "^1.0.20",
		"log4js": "^6.2.1",
		"superagent": "^5.2.2",
		"urllib-sync": "^1.1.4"
	}
}
